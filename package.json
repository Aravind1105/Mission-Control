    "react-bootstrap": "^0.32.4",
    "react-country-flag": "^1.0.1",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.5.2",
    "react-feather": "^1.1.4",
    "react-intl": "^2.6.0",
    "react-perfect-scrollbar": "^1.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.4.7",
    "reactstrap": "^6.4.0",
    "redux": "^4.0.0",
    "redux-first-router": "^0.0.16-next",
    "redux-first-router-link": "^1.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
