{
  "name": "livello-mission-control",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.3",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-intl": "^2.6.0",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.5",
    "reactstrap": "^6.4.0",
    "redux": "^4.0.0",
    "redux-first-router": "^0.0.16-next",
    "redux-first-router-link": "^1.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-css": "stylus -c src/assets/stylus/ --out src/assets/css",
    "watch-css": "npm run build-css && stylus -c -w src/assets/stylus/ --out src/assets/css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
